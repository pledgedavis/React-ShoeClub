ents/ShoeList.js",["85","86"],"/Users/pledge/Development/react-project/frontend/src/containers/Contact.js",[],"/Users/pledge/Development/react-project/frontend/src/components/AboutMe.js",[],"/Users/pledge/Development/react-project/frontend/src/components/ContactMe.js",[],"/Users/pledge/Development/react-project/frontend/src/components/Navbar.js",[],"/Users/pledge/Development/react-project/frontend/src/containers/Shoes.js",[],"/Users/pledge/Development/react-project/frontend/src/store.js",[],"/Users/pledge/Development/react-project/frontend/src/components/ShoePage.js",["87","88"],"import React, { useState, useEffect } from \"react\";\n// the above hook useState allows me to have state variables\nimport { getSingleShoe } from \"../store/actions/shoesAction\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { addComment } from \"../store/actions/commentsAction.js\";\n\nexport default function ShoePage({ location }) {\n  // location is a default prop whos parent is the router\n  const [description, setDescription] = useState(\"\");\n  const [showComments, setShowComments] = useState([]);\n  // functions to update the state\n  // debugger;\n  const shoeSelected = useSelector((state) => state.shoeList.shoe);\n  // the above function updates this part of the state ^\n  const dispatch = useDispatch();\n  // used to dispatch actions to the store\n\n  //console.log(\"one shoe?\", location?.shoe);\n  let comments;\n  // debugger;\n\n  useEffect(() => {\n    // runs on page load\n    if (location.shoe) {\n      dispatch(getSingleShoe(location.shoe.id));\n    }\n  }, [getSingleShoe, dispatch]);\n\n  useEffect(() => {\n    console.log(\"shoes effect?\", shoeSelected);\n    if (shoeSelected) {\n      comments = shoeSelected.comments;\n      const updatedComments = comments.map((comment) => {\n        const { description, id } = comment;\n        return <p key={id}>{description}</p>;\n      });\n      setShowComments(updatedComments);\n      setDescription(\"\");\n      // set description clears form after comments are updated\n    }\n  }, [shoeSelected]);\n  // dependency array so useEffect can keep track of the functions objects/variables\n  // debugger;\n\n  if (!location.shoe) {\n    return <h1> No shoe selected</h1>;\n  }\n\n  const handleSubmit = (event) => {\n    event.preventDefault();\n\n    const newComment = {\n      description,\n      shoe_id: location.shoe.id,\n    };\n    // creating a newComment prop so a user is able to create a comment\n    console.log(\"dispatching comments action\");\n    dispatch(addComment(newComment));\n  };\n\n  console.log(\"single shoe from state\", shoeSelected);\n\n  return (\n    <div>\n      <h1> Comment Page </h1>\n      <h4> Leave your comments below</h4>\n      <hr></hr>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      {showComments}\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n\n      <form onSubmit={handleSubmit}>\n        <input\n          type=\"text\"\n          value={description}\n          onChange={(event) => setDescription(event.target.value)}\n          placeholder=\"comment\"\n        />\n        <input type=\"submit\" value=\"Submit\" />\n      </form>\n    </div>\n  );\n}\n","/Users/pledge/Development/react-project/frontend/src/store/reducers/shoeReducer.js",[],"/Users/pledge/Development/react-project/frontend/src/constant/ActionTypes.js",[],"/Users/pledge/Development/react-project/frontend/src/store/actions/shoesAction.js",[],"/Users/pledge/Development/react-project/frontend/src/store/actions/commentsAction.js",[],"/Users/pledge/Development/react-project/frontend/src/components/LikeShoeButton.js",["89"],"import React, { useState, Component } from \"react\";\n\n// function LikeShoeButton() {\n//   const [count, setCount] = useState(0);\n\n//   return (\n// <>\n//   <button onClick={() => setCount(count + 1)}>\n//     Like!\n//   </button>\n//   <p>{count}</p>\n// </>\n//   );\n// }\n\nclass LikeShoeButton extends Component {\n  state = {\n    count: 0,\n  };\n\n  render() {\n    const { count } = this.state;\n    return (\n      <>\n        <button onClick={() => this.setState({ count: count + 1 })}>\n          Like!\n        </button>\n        <h2>{count}</h2>\n      </>\n    );\n  }\n}\n\nexport default LikeShoeButton;\n",{"ruleId":"90","severity":1,"message":"91","line":3,"column":10,"nodeType":"92","messageId":"93","endLine":3,"endColumn":17},{"ruleId":"90","severity":1,"message":"94","line":4,"column":35,"nodeType":"92","messageId":"93","endLine":4,"endColumn":41},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"90","severity":1,"message":"99","line":4,"column":10,"nodeType":"92","messageId":"93","endLine":4,"endColumn":15},{"ruleId":"100","severity":1,"message":"101","line":68,"column":11,"nodeType":"102","endLine":68,"endColumn":56},{"ruleId":"103","severity":1,"message":"104","line":27,"column":6,"nodeType":"105","endLine":27,"endColumn":31,"suggestions":"106"},{"ruleId":"103","severity":1,"message":"107","line":32,"column":18,"nodeType":"108","endLine":32,"endColumn":39},{"ruleId":"90","severity":1,"message":"109","line":1,"column":17,"nodeType":"92","messageId":"93","endLine":1,"endColumn":25},"no-unused-vars","'connect' is defined but never used.","Identifier","unusedVar","'Switch' is defined but never used.","no-native-reassign",["110"],"no-negated-in-lhs",["111"],"'Route' is defined but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'location.shoe'. Either include it or remove the dependency array. Outer scope values like 'getSingleShoe' aren't valid dependencies because mutating them doesn't re-render the component.","Array