onents/Navbar\";\nimport ShoePage from \"./components/ShoePage\";\n\nclass App extends Component {\n  render() {\n    return (\n      <Router>\n        <Navbar />\n        {/* <Switch> */}\n        <Route exact path=\"/\" component={Welcome} />\n        <Route exact path=\"/shoes\" component={Shoes} />\n        <Route exact path=\"/contact\" component={Contact} />\n        <Route exact path=\"/shoePage\" component={ShoePage} />\n        {/* </Switch> */}\n      </Router>\n    );\n  }\n}\nexport default App;\n",["84","85"],"/Users/pledge/Development/react-project/frontend/src/index.js",[],"/Users/pledge/Development/react-project/frontend/src/reportWebVitals.js",[],"/Users/pledge/Development/react-project/frontend/src/containers/Welcome.js",[],"/Users/pledge/Development/react-project/frontend/src/components/WelcomeAbout.js",[],"/Users/pledge/Development/react-project/frontend/src/components/WelcomeHeader.js",[],"/Users/pledge/Development/react-project/frontend/src/components/ShoeList.js",["86","87","88","89","90"],"/Users/pledge/Development/react-project/frontend/src/containers/Contact.js",[],"/Users/pledge/Development/react-project/frontend/src/components/AboutMe.js",[],"/Users/pledge/Development/react-project/frontend/src/components/ContactMe.js",[],"/Users/pledge/Development/react-project/frontend/src/components/Navbar.js",[],"/Users/pledge/Development/react-project/frontend/src/containers/Shoes.js",[],"/Users/pledge/Development/react-project/frontend/src/store.js",[],"/Users/pledge/Development/react-project/frontend/src/components/ShoePage.js",["91","92"],"import React, { useState, useEffect } from \"react\";\n// the above hook useState allows me to have state variables\nimport { getSingleShoe } from \"../store/actions/shoesAction\";\nimport { useSelector, useDispatch } from \"react-redux\";\nimport { addComment } from \"../store/actions/commentsAction.js\";\n\nexport default function ShoePage({ location }) {\n  const [description, setDescription] = useState(\"\");\n  const [showComments, setShowComments] = useState([]);\n  // functions to update the state\n  // debugger;\n  const shoeSelected = useSelector((state) => state.shoeList.shoe);\n  // the above function updates this part of the state ^\n  const dispatch = useDispatch();\n  // used to dispatch actions to the store\n\n  //console.log(\"one shoe?\", location?.shoe);\n  let comments;\n  // debugger;\n\n  useEffect(() => {\n    // runs on page load\n    if (location.shoe) {\n      dispatch(getSingleShoe(location.shoe.id));\n    }\n  }, [getSingleShoe, dispatch]);\n\n  useEffect(() => {\n    console.log(\"shoes effect?\", shoeSelected);\n    if (shoeSelected) {\n      comments = shoeSelected.comments;\n      const updatedComments = comments.map((comment) => {\n        const { description, id } = comment;\n        return <p key={id}>{description}</p>;\n      });\n      setShowComments(updatedComments);\n      setDescription(\"\");\n      // set description clears form after comments are updated\n    }\n  }, [shoeSelected]);\n  // dependency array so useEffect can keep track of the functions objects/variables\n  // debugger;\n\n  if (!location.shoe) {\n    return <h1> No shoe selected</h1>;\n  }\n\n  const handleSubmit = (event) => {\n    event.preventDefault();\n\n    const newComment = {\n      description,\n      shoe_id: location.shoe.id,\n    };\n    // creating a newComment prop so a user is able to create a comment\n    console.log(\"dispatching comments action\");\n    dispatch(addComment(newComment));\n  };\n\n  console.log(\"single shoe from state\", shoeSelected);\n\n  return (\n    <div>\n      <h1> Comment Page </h1>\n      <h4> Leave your comments below</h4>\n      <hr></hr>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      {showComments}\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n      <br></br>\n\n      <form onSubmit={handleSubmit}>\n        <input\n          type=\"text\"\n          value={description}\n          onChange={(event) => setDescription(event.target.value)}\n          placeholder=\"comment\"\n        />\n        <input type=\"submit\" value=\"Submit\" />\n      </form>\n    </div>\n  );\n}\n","/Users/pledge/Development/react-project/frontend/src/store/reducers/shoeReducer.js",[],"/Users/pledge/Development/react-project/frontend/src/constant/ActionTypes.js",[],"/Users/pledge/Development/react-project/frontend/src/store/actions/shoesAction.js",[],"/Users/pledge/Development/react-project/frontend/src/store/actions/commentsAction.js",[],"/Users/pledge/Development/react-project/frontend/src/components/LikeShoeButton.js",["93"],"import React, { useState, Component } from \"react\";\n\n// function LikeShoeButton() {\n//   const [count, setCount] = useState(0);\n\n//   return (\n// <>\n//   <button onClick={() => setCount(count + 1)}>\n//     Like!\n//   </button>\n//   <p>{count}</p>\n// </>\n//   );\n// }\n\nclass LikeShoeButton extends Component {\n  state = {\n    count: 0,\n  };\n\n  render() {\n    const { count } = this.state;\n    return (\n      <>\n        <button onClick={() => this.setState({ count: count + 1 })}>\n          Like!\n        </button>\n        <p>{count}</p>\n      </>\n    );\n  }\n}\n\nexport default LikeShoeButton;\n",{"ruleId":"94","severity":1,"message":"95","line":3,"column":10,"nodeType":"96","messageId":"97","endLine":3,"endColumn":17},{"ruleId":"94","severity":1,"message":"98","line":4,"column":35,"nodeType":"96","messageId":"97","endLine":4,"endColumn":41},{"ruleId":"99","replacedBy":"100"},{"ruleId":"101","replacedBy":"102"},{"ruleId":"94","severity":1,"message":"103","line":1,"column":17,"nodeType":"96","messageId":"97","endLine":1,"endColumn":25},{"ruleId":"94","severity":1,"message":"104","line":4,"column":10,"nodeType":"96","messageId":"97","endLine":4,"endColumn":15},{"ruleId":"94","severity":1,"message":"105","line":1